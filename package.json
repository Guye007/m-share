{
  "name": "m-share",
  "version": "1.0.0",
  "description": "h5分享",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "http-server -c-1 -p 8000 ./",
    "dev": "rollup src/index.js --watch --output.format umd --name \"Mshare\" --output.file dist/m-share.js",
    "build": "npm run build:umd && npm run build:es6 && npm run build:iife",
    "build:umd": "rollup src/index.js --output.format umd --name \"Mshare\" --output.file dist/m-share.js",
    "build:es6": "rollup src/index.js --output.format es --name \"Mshare\" --output.file dist/m-share-es6.js",
    "build:iife": "rollup src/index.js --output.format iife --name \"Mshare\" --output.file dist/m-share-iife.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/backToNature/m-share.git"
  },
  "author": "'guohao' <'backtonature@foxmail.com'>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/backToNature/m-share/issues"
  },
  "homepage": "https://github.com/backToNature/m-share#readme",
  "devDependencies": {
    "http-server": "^0.11.1",
    "rollup": "^0.59.2"
  }
}
